---
apiVersion: noobaa.io/v1alpha1
kind: NooBaa
metadata:
  name: noobaa
  labels:
    app: openshift-migration-noobaa
  namespace: {{ noobaa_system_namespace }}
spec:
 dbResources:
   requests:
     cpu: {{ noobaa_system_db_cpu }}
     memory: {{ noobaa_system_db_mem }}
 coreResources:
   requests:
     cpu: {{ noobaa_system_core_cpu }}
     memory: {{ noobaa_system_core_mem }}
